{
  "success": @boolean@,
  "result": {
      "id": "@integer@.greaterThan(0)",
      "title": "@string@",
      "description": "@string@||@null@",
      "episodeNumber": "@integer@.greaterThan(0)",
      "podcast": {
        "id": "@integer@.greaterThan(0)",
        "name": "@string@",
        "createdAt": "@string@.isDateTime()",
        "updatedAt": "@string@.isDateTime()"
      },
      "downloadUrl": "@string@.isUrl()||@null@",
      "trackLength": "@integer@.greaterThan(0)||@null@",
      "bitRate": "@integer@.greaterThan(0)||@null@",
      "sampleRate": "@integer@.greaterThan(0)||@null@",
      "channels": "@integer@.greaterThan(0)||@null@",
      "isVariableBitRate": "@boolean@||@null@",
      "isLossless": "@boolean@||@null@",
      "mimeType": "@string@||@null@",
      "fileSize": "@integer@.greaterThan(0)||@null@",
      "createdAt": "@string@.isDateTime()",
      "updatedAt": "@string@.isDateTime()"
  }
}
